<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/win"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/input"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="输入事件" />

    <Button
        android:id="@+id/fgBroadcast"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="前台广播" />

    <Button
        android:id="@+id/bgBroadcast"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="后台广播" />

    <Button
        android:id="@+id/fgSer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="前台服务" />

    <Button
        android:id="@+id/bgSer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="后台服务" />
</LinearLayout>